{
  "name": "serverless-graphql-apollo",
  "version": "0.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:serverless/serverless-graphql-apollo.git"
  },
  "scripts": {
    "debug": "node --inspect=0.0.0.0:5858 --nolazy node_modules/.bin/serverless offline --webpack-use-polling",
    "start": "yarn && env-cmd ./config/security.env.local serverless offline start --region=us-east-1 --stage=staging --webpack-use-polling=500 --debugOffline",
    "prod": "yarn && env-cmd ./config/security.env.local serverless offline start --region=us-east-1 --stage=staging --webpack-use-polling=500 --debugOffline --stage=production",
    "precommit": "lint-staged",
    "lint": "eslint *.js",
    "prettier": "prettier --no-semi --write \"*.{js,css,json}\"",
    "prettier-check": "prettier --list-different \"*.{js,css,json}\"",
    "test": "yarn run lint && yarn run prettier-check"
  },
  "lint-staged": {
    "*.{js,json,css}": [
      "prettier --write",
      "git add"
    ]
  },
